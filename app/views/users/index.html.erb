<h1>Users!</h1>
<table class="table table-striped table-bordered">
  <thead>
    <th>Username</th>
    <th>Blog</th>
    <th>Profile</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= link_to "User Blog!", user_posts_path(user) %></td>
        <td><%= link_to "Profile!", user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

