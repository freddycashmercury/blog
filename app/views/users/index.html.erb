<h1>Users!</h1>
<table class="table table-striped table-bordered">
  <thead>
    <th>Username</th>
    <th>Blog</th>
    <th>Profile</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <% if !user.feed.blank? %>
          <td><%= link_to "User Blog!", user_feed_path(user, user.feed) %></td>
        <% else %>
           <td>PLACEHOLDER</td>
         <% end %>
        <td><%= link_to "Profile!", user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

