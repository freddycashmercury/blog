<%= form_for([@user, @feed]) do |f| %>
  <div class="container">
    <div class="form-group">
      <%= f.label :name, :class => "col-sm-2 control-label" %>
      <%= f.text_field :name, :value => @feed.name, :class => "form-control" %>
    </div>
    <div class="form-group">
      <%= f.label :description, :class => "col-sm-2 control-label" %>
      <%= f.text_area :description, :value => @feed.description, :class => "form-control" %>
    </div>
  <%= f.submit "Submit", :class => "submit-button" %>
  </div>
<% end %>
